package pessoadois;

/**
 *
 * @author IFFarroupilha
 */
public class PessoaUm {
            String nome;
            int anonasc;
            PessoaUm pai;
            
        PessoaUm (String nomep){
            nome = nomep;
            anonasc = 2000;
        }
        PessoaUm (String nomep,int anop){
            nome = nomep;
            anonasc = anop;
        }
        PessoaUm (String nomea,int anoa, PessoaUm pai){
            nome = nomea;
            anonasc = anoa;
            pai = pai;
        }
}
